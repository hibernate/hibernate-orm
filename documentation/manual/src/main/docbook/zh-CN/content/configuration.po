"        &lt;class-cache class=\"org.hibernate.auction.Bid\" usage=\"read-only"
"\"/&gt;\n"
"        &lt;collection-cache collection=\"org.hibernate.auction.Item.bids\" "
"usage=\"read-write\"/&gt;\n"
"\n"
"    &lt;/session-factory&gt;\n"
