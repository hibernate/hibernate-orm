          <xs:enumeration value="read"/>
          <xs:enumeration value="upgrade"/>
          <xs:enumeration value="upgrade-nowait"/>
          <xs:enumeration value="write"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="return-scalar">
    <xs:complexType>
      <xs:attributeGroup ref="mapping:attlist.return-scalar"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.return-scalar">
    <xs:attribute name="column" use="required"/>
    <xs:attribute name="type"/>
  </xs:attributeGroup>
  <xs:element name="synchronize">
    <xs:complexType>
      <xs:attributeGroup ref="mapping:attlist.synchronize"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.synchronize">
