    Hibernate dialect implementation.
  -->
  <xs:element name="dialect-scope">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="mapping:attlist.dialect-scope"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.dialect-scope">
    <xs:attribute name="name" use="required"/>
  </xs:attributeGroup>
