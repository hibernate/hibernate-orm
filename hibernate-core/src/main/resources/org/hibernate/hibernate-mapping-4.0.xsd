  <!-- The sql-query element declares a named SQL query string -->
  <xs:element name="sql-query">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="mapping:return-scalar"/>
