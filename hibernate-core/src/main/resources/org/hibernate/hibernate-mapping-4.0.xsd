      <xs:attributeGroup ref="mapping:attlist.param"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.param">
    <xs:attribute name="name" use="required"/>
  </xs:attributeGroup>
  <!--
    The column element is an alternative to column attributes and required for 
