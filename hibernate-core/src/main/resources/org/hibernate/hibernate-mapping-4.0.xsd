  <xs:attributeGroup name="attlist.many-to-any">
    <xs:attribute name="id-type" use="required"/>
    <xs:attribute name="meta-type"/>
  </xs:attributeGroup>
  <!-- - default: Hibernate.CLASS -->
  <xs:element name="index-many-to-any">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mapping:column-element"/>
        <xs:element maxOccurs="unbounded" ref="mapping:column-element"/>
      </xs:sequence>
      <xs:attributeGroup ref="mapping:attlist.index-many-to-any"/>
