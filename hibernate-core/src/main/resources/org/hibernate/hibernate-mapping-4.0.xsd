        <xs:element ref="mapping:return"/>
        <xs:element ref="mapping:return-join"/>
        <xs:element ref="mapping:load-collection"/>
      </xs:choice>
      <xs:attributeGroup ref="mapping:attlist.resultset"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.resultset">
    <xs:attribute name="name" use="required"/>
