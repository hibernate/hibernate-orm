    <xs:attribute name="type" use="required"/>
  </xs:attributeGroup>
  <!-- The resultset element declares a named resultset mapping definition for SQL queries -->
  <xs:element name="resultset">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
