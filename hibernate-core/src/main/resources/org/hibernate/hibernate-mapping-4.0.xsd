  <xs:attributeGroup name="attlist.sql-query">
    <xs:attribute name="name" use="required"/>
    <xs:attribute name="resultset-ref"/>
    <xs:attribute name="flush-mode">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="auto"/>
          <xs:enumeration value="never"/>
          <xs:enumeration value="always"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="cacheable" default="false">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="cache-region"/>
    <xs:attribute name="fetch-size"/>
    <xs:attribute name="timeout"/>
    <xs:attribute name="cache-mode">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="get"/>
          <xs:enumeration value="ignore"/>
          <xs:enumeration value="normal"/>
          <xs:enumeration value="put"/>
          <xs:enumeration value="refresh"/>
        </xs:restriction>
      </xs:simpleType>
