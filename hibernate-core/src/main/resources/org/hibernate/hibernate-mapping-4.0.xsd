  <xs:attributeGroup name="attlist.return">
    <xs:attribute name="alias"/>
    <xs:attribute name="entity-name"/>
    <xs:attribute name="class"/>
    <xs:attribute name="lock-mode" default="read">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="none"/>
          <xs:enumeration value="read"/>
          <xs:enumeration value="upgrade"/>
          <xs:enumeration value="upgrade-nowait"/>
          <xs:enumeration value="write"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="return-property">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mapping:return-column"/>
      </xs:sequence>
