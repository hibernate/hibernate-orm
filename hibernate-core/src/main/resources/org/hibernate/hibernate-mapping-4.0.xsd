  -->
  <xs:element name="many-to-any">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mapping:meta-value"/>
        <xs:element ref="mapping:column-element"/>
        <xs:element maxOccurs="unbounded" ref="mapping:column-element"/>
