  <!--
    Element for defining "auxiliary" database objects.  Must be one of two forms:
    
    #1 :
        <database-object>
            <definition class="CustomClassExtendingAuxiliaryObject"/>
        </database-object>
    
    #2 :
        <database-object>
            <create>CREATE OR REPLACE ....</create>
            <drop>DROP ....</drop>
        </database-object>
  -->
  <xs:element name="database-object">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="mapping:definition"/>
          <xs:sequence>
            <xs:element ref="mapping:create"/>
            <xs:element ref="mapping:drop"/>
          </xs:sequence>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mapping:dialect-scope"/>
      </xs:sequence>
    </xs:complexType>
