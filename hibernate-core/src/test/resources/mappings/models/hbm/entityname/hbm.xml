<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
   This was supported in hbm.xl, but is not supported in mapping.xml...
-->
<hibernate-mapping package="org.hibernate.orm.test.boot.models.hbm.entityname">
    <class name="Address" entity-name="BillingAddress">
        <id name="id" type="integer"/>
        <property name="txt" type="string"/>
    </class>
    <class name="Address" entity-name="ShippingAddress">
        <id name="id" type="integer"/>
        <property name="txt" type="string"/>
    </class>
</hibernate-mapping>