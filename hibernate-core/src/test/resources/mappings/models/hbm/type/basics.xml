<entity-mappings xmlns="http://www.hibernate.org/xsd/orm/mapping"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 version="7.0">
    <entity class="org.hibernate.testing.orm.domain.gambit.EntityOfBasics" access="FIELD" metadata-complete="true">
        <attributes>
            <id name="id"/>
            <basic name="theBoolean"/>
            <basic name="theString"/>
            <basic name="theInt"/>
            <basic name="theInteger"/>
            <basic name="theShort"/>
            <basic name="theDouble"/>
            <basic name="theUrl"/>
            <basic name="theClob"/>
            <basic name="theInstant"/>
            <basic name="theDate">
                <temporal>DATE</temporal>
            </basic>
            <basic name="theTime">
                <temporal>TIME</temporal>
            </basic>
            <basic name="theTimestamp"/>
        </attributes>
    </entity>
</entity-mappings>
