<entity-mappings xmlns="http://www.hibernate.org/xsd/orm/mapping"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 version="7.0">
    <entity name="SimpleDynamicEntity" metadata-complete="true">
        <table name="hbm_simple_entity_dynamic"/>
        <attributes>
            <id name="id">
                <target>Integer</target>
            </id>
            <basic name="name">
                <target>String</target>
            </basic>
        </attributes>
    </entity>
</entity-mappings>