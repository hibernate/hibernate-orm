<entity-mappings xmlns="http://www.hibernate.org/xsd/orm/mapping" version="7.0">
    <package>org.hibernate.orm.test.cascade</package>

    <entity class="DeleteOrphanChild">
        <table name="DeleteOrphanChild"/>
        <attributes>
            <id name="id">
                <column name="id"/>
                <generated-value generator="native"/>
                <type value="Long"/>
            </id>
            <many-to-one name="parent" target-entity="Parent" optional="false">
                <cascade>
                    <cascade-all/>
                </cascade>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
