<entity-mappings xmlns="http://www.hibernate.org/xsd/orm/mapping"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 version="7.0">
    <package>org.hibernate.orm.test.cascade</package>

    <entity class="DeleteOrphanChild">
        <attributes>
            <id name="id">
                <generated-value generator="increment"/>
            </id>
            <many-to-one name="parent" fetch="LAZY">
                <join-column name="parent_fk"/>
                <cascade>
                    <cascade-all/>
                </cascade>
            </many-to-one>
        </attributes>
    </entity>

</entity-mappings>
