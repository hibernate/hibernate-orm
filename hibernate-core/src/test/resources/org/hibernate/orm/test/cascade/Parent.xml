<entity-mappings xmlns="http://www.hibernate.org/xsd/orm/mapping"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 version="7.0">
    <package>org.hibernate.orm.test.cascade</package>

    <entity class="Parent">
        <attributes>
            <id name="id">
                <generated-value generator="increment"/>
            </id>
            <one-to-many name="children" mapped-by="parent">
                <cascade>
                    <cascade-all/>
                </cascade>
            </one-to-many>
            <one-to-many name="deleteOrphanChildren" mapped-by="parent" orphan-removal="true">
                <cascade>
                    <cascade-all/>
                </cascade>
            </one-to-many>
        </attributes>
    </entity>

</entity-mappings>

