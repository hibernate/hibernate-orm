<entity-mappings xmlns="http://www.hibernate.org/xsd/orm/mapping" version="7.0">
    <package>org.hibernate.orm.test.cascade</package>
    <entity class="Parent">
        <table name="Parent"/>
        <attributes>
            <id name="id">
                <column name="id"/>
                <generated-value generator="assigned"/>
                <type value="Long"/>
            </id>
            <one-to-many name="children" target-entity="Child" mapped-by="parent">
                <cascade>
                    <cascade-all/>
                </cascade>
            </one-to-many>
            <transient name="deleteOrphanChildren"/>
        </attributes>
    </entity>

</entity-mappings>
